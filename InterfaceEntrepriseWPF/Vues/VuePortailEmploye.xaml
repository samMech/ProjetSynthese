<UserControl x:Class="InterfaceEntrepriseWPF.Vues.VuePortailEmploye"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InterfaceEntrepriseWPF.Vues"
             mc:Ignorable="d" Height="340" Width="580">
    <DockPanel LastChildFill="True">       
        <Grid DockPanel.Dock="Bottom">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="btnConsulterRDV" Content="Consulter vos rendez-vous" Grid.Column="0" Margin="10,0"/>
            <Button x:Name="btnGererRDV" Content="Gérer vos disponibilité" Grid.Column="1" Margin="10,0"/>
        </Grid>
        <Grid>
        </Grid>
    </DockPanel>
</UserControl>
